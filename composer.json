{
    "name": "sndpbag/blog",
    "description": "A simple blog package for Laravel.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sndp bag",
            "email": "sndpbagg@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
         "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Sndpbag\\Blog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sndpbag\\Blog\\Providers\\BlogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}